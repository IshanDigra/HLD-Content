    {
      "id": "Y3jegDnt_wNByJWdgpEyA",
      "type": "text",
      "x": 1316.9821143674592,
      "y": 103.5821688933911,
      "width": 1100.31982421875,
      "height": 1000,
      "angle": 0,
      "strokeColor": "#1e1e1e",
      "backgroundColor": "transparent",
      "fillStyle": "solid",
      "strokeWidth": 1,
      "strokeStyle": "solid",
      "roughness": 0,
      "opacity": 100,
      "groupIds": [],
